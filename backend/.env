GOOGLE_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# File Upload Configuration
MAX_FILE_SIZE=52428800  # 50MB in bytes
UPLOAD_DIR=./uploads

# Data Storage
DATA_DIR=./data

# RAG Configuration
RAG_CHUNK_SIZE=500
RAG_CHUNK_OVERLAP=50
RAG_TOP_K=5
RAG_MIN_SIMILARITY=0.1

# Search Configuration
TFIDF_MAX_FEATURES=5000
SEMANTIC_MIN_SIMILARITY=0.2

# Chat Configuration
CHAT_MAX_CONTEXT_LENGTH=8000
CHAT_TEMPERATURE=0.7

# Logging
LOG_LEVEL=INFO
LOG_FILE=ragflow_backend.log

# CORS (Development)
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Development
DEBUG=false
RELOAD=false
